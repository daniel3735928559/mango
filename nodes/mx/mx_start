export MC_ADDR='tcp://localhost:'"$1"
export PYTHONPATH='/home/zoom/suit/mango/libmango/py'
export MANGO_ID=$2
python mx_agent.py &
export MX_PID=$!
echo $MX_PID
while [[ ! -f .run/$MX_PID && ! -p .run/"$MX_PID"_out ]]; do
  [[ -f .run/"$MX_PID"_out ]] && echo hello;
  [[ ! -f .run/"$MX_PID"_out ]] && echo hello2;
  printf '.';
  sleep 1;
  done
bash --init-file .run/$MX_PID
