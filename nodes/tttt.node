[config]
name tttt
command 'python -u ./py/nodes/tttt/tttt.py'
env PYTHONPATH ./libmango/py

[interface]
input move {turn:string,bx:num,by:num,x:num,y:num}
input rules {}
input state {}
input restart {turn:string}
output state {turn:string,board:[[[[string]]]],boardstr:string,nextboard:string}
output message {text:string}