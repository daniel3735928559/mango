[config]

[nodes]
excite_py py
excite_c c
excite_js js
excite_fe frontend --MU_HTTP_PORT=11702
split splitter
merge merger


[routes]
frontend > splitter
splitter > merger orig
splitter > py > merger py
splitter > c > merger c
splitter > js > merger js
merger orig py c js > r excited {str:"orig="+orig.str + " py="+py.str  + " c="+c.str  + " js="+js.str } > frontend
