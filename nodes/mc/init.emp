[config]

[nodes]
mx shell
split s1
jam j
merge m1
excite_py ex
excite_fe ex_fe --MU_HTTP_PORT=11702
mc_fe mc_fe --MU_HTTP_PORT=11703

[routes]
#shell <> mc
#shell | f excite | ex | shell
shell <> j
ex_fe > s1
mc <> mc_fe
s1 > ex > m1 excited
s1 > m1 unexcited
m1 excited unexcited > r excited {str:excited.str + "|" + unexcited.str} > ex_fe
